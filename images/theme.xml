<synth>
  <style id="default">
    <opaque value="true"/>
    <state>
      <color type="BACKGROUND" value="#DDDDDD" />
      <color type="FOREGROUND" value="#000000" />
      <color type="TEXT_FOREGROUND" value="#111111" />
    </state>
    <font name="Lucida" size="12"/>
  </style>

  <style id="accent1">
    <opaque value="true"/>
    <state>
        <color type="BACKGROUND" value="#36425F" />
        <color type="FOREGROUND" value="#FFFFFF" />
        <color type="TEXT_FOREGROUND" value="#111111" />
    </state>
    <state value="PRESSED">
        <color type="BACKGROUND" value="#153642" />
    </state>
  </style>

  <style id="accent2">
    <opaque value="true"/>
    <property key="Button.iconTextGap" type="integer" value="5" />
    <state>
        <color type="BACKGROUND" value="#A3351E" />
        <color type="FOREGROUND" value="#FFFFFF" />
        <color type="TEXT_FOREGROUND" value="#111111" />
    </state>
    <state value="PRESSED">
        <color type="BACKGROUND" value="#252525" />
        <color type="TEXT_FOREGROUND" value="#DDDDDD" />
    </state>
  </style>

  <style id="accent3">
    <opaque value="true"/>
    <state>
        <color type="BACKGROUND" value="#7A7A7A" />
        <color type="FOREGROUND" value="#FFFFFF" />
        <color type="TEXT_FOREGROUND" value="#111111" />
    </state>
    <state value="PRESSED">
        <color type="BACKGROUND" value="#C2C2C2" />
    </state>
  </style>

  <style id="content1_bg">
    <opaque value="true"/>
    <state>
        <color type="BACKGROUND" value="#FFFFFF" />
    </state>
  </style>

  <style id="content1_select">
    <state value="SELECTED">
        <color type="BACKGROUND" value="#DDDDEE" />
    </state>
    <state value="FOCUSED">
        <color type="BACKGROUND" value="#EEDDDD" />
    </state>
    <state value="MOUSE_OVER">
        <color type="BACKGROUND" value="#DDEEDD" />
    </state>
  </style>

  <style id="content2_bg">
    <opaque value="true"/>
    <state>
        <color type="BACKGROUND" value="#F6F8FA" />
    </state>
  </style>

  <style id="content3_bg">
    <opaque value="true"/>
    <state>
        <color type="BACKGROUND" value="#F9B60E" />
    </state>
  </style>

  <bind style="default" type="region" key="Panel"/>
  <bind style="content1_bg" type="region" key="List"/>
  <bind style="accent2" type="region" key="button"/>
  <bind style="content2_bg" type="region" key="SCROLLBAR"/>
  <bind style="accent2" type="region" key="SCROLLBARTHUMB"/>
  <bind style="content2_bg" type="region" key="SCROLLBARTRACK"/>
</synth>
